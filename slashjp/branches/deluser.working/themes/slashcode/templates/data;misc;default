__section__
default
__description__
Repository for random data elements.

* value = the name of the data element to retrieve

Each data element may have any number of other variables.
__title__

__page__
misc
__lang__
en_US
__name__
data
__template__
[% SWITCH value %]

[% CASE 'tablebegin' %]
	[% returnme.data_constant = 1 %] 
[% CASE 'tableend' %]
	[% returnme.data_constant = 1 %] 
[% CASE 'cagebegin' %]
	[% returnme.data_constant = 1 %] 
[% CASE 'cageend' %]
	[% returnme.data_constant = 1 %] 
[% CASE 'commentbegin' %]
	[% returnme.data_constant = 1 %] 
	<li>
[% CASE 'commentend' %]
	[% returnme.data_constant = 1 %] 
	</li>
[% CASE 'indentbegin' %]
	[% returnme.data_constant = 1 %] 
	<ul>
[% CASE 'indentend' %]
	[% returnme.data_constant = 1 %] 
	</ul>
[% CASE 'cagebigbegin' %]
	[% returnme.data_constant = 1 %] 
[% CASE 'cagebigend' %]
	[% returnme.data_constant = 1 %] 
[% CASE 'fulllistbegin' %]
        [% returnme.data_constant = 1 %]
	<!-- "commentlist" will most likely change -->
        <ul id="commentlist">
[% CASE 'fulllistend' %]
        [% returnme.data_constant = 1 %]
        </ul>
[% CASE 'fullcommentbegin' %]
        [% returnme.data_constant = 1 %]
        <li class="comment">
[% CASE 'fullcommentend' %]
        [% returnme.data_constant = 1 %]
        </li>
[% CASE 'displayThreadLink' %]
	[% foo = hidden == 1 ? 'y' : 'ies'; hidden %] repl[% foo %]
[% CASE 'atonish' %]
	[% returnme.data_constant = 1 %] 
	around
[% CASE 'aton' %]
	[% returnme.data_constant = 1 %] 
	on
[% CASE 'hour' %]
	[% returnme.data_constant = 1 %] 
	hour
[% CASE 'hours' %]
	[% returnme.data_constant = 1 %] 
	hours
[% CASE 'minute' %]
	[% returnme.data_constant = 1 %] 
	minute
[% CASE 'minutes' %]
	[% returnme.data_constant = 1 %] 
	minutes
[% CASE 'second' %]
	[% returnme.data_constant = 1 %] 
	second
[% CASE 'seconds' %]
	[% returnme.data_constant = 1 %] 
	seconds

[% CASE 'SLASH-UNKNOWN-TAG' %]
<b>[Unknown Slash Tag [% tag %]]</b>

[% CASE 'SLASH-UNKNOWN-IMAGE' %]
	[% returnme.data_constant = 1 %] 
	<b>[Unknown Image]</b>

[% CASE 'SLASH-UNKNOWN-STORY' %]
	[% returnme.data_constant = 1 %] 
	<b>[Unknown Story]</b>

[% CASE 'SLASH-UNKNOWN-USER' %]
	[% returnme.data_constant = 1 %] 
	<b>[Unknown User]</b>

