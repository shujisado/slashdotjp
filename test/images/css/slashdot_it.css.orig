a {color: #898163}
div#topnav { background: #898163 url(//images.slashdot.org/topnav-bg-it.png) repeat-x left top; }
  div#topnav div#logo h1 a { background: url(//images.slashdot.org/logo-it.png) no-repeat left top; }
div.article div.title,
div.generaltitle div.title,
div.commentTop div.title
{ background: #898163 url(//images.slashdot.org/article-title-bg-it.png) repeat-x left top; }
  div.article div.title h3,
  div.generaltitle div.title h3,
  div.commentTop div.title h4
  { background: url(//images.slashdot.org/article-title-left-it.png) no-repeat left top; }
    div.article div.title span,
    div.article div.title span a { color: #eed; }
div#links div.block div.content ul li a:hover {background: #898163 url(//images.slashdot.org/link-arrow.gif) no-repeat right center; color: #fff;}
div#links div.block div.content, div#slashboxes div.block div.content { background: #eed url(//images.slashdot.org/slashbox-bottom-left-it.png) no-repeat left bottom; }
div#links div.block div.title, div#slashboxes div.block div.title {background: #898163 url(//images.slashdot.org/article-title-bg-it.png)}
div#links div.block {
  background: #898163 url(//images.slashdot.org/article-title-bg-it.png) repeat-x left top;
}
div#links div.block div#links-sections-title{
  background: url(//images.slashdot.org/block-title-right-it.png) no-repeat right top;
}
div#slashboxes div.block div.title h4 {
      background: url(//images.slashdot.org/block-title-right-it.png) no-repeat right top;
    }

  div#links div.block div.content ul { border-bottom: 1px solid #fff; }
    div#links div.block div.content ul li a { border-top: 1px solid #fff;}
.data_head, .barColor, .tb_col, , .thor .commentControlFooter { background: #898163; }
.date_head { color: #fff; }
.comment > .oneline .commentTop .title h4 a { color: #898163; }
div.commentSub { border-bottom: 1px solid #898163; }


//firehose related css begin
.story, .story a  {color: #fff !important;}
.story .date, .story a.skin {color: #ffffff !important;}
#firehose .article div.tagtitleclosed span span, #firehose .article div.tagtitleopen span span, #firehose .generaltitle h3 a, #firehose div.tags div.tagtitleclosed a, #firehose div.tags div.tagtitleclosed, #firehose div.tags div.tagtitleopen a, #firehose div.tags div.tagtitleopen {color: #898163 !important}
#firehose div.briefarticle .story div.title h3, #firehose div.briefarticle .story div.title, #firehose div.article .story div.title h3, #firehose div.article .story div.title, .firemenu div.title, .firemenu div.title h3
{ background: #004 url(//images.slashdot.org/article-title-bg-it.png) repeat-x left top !important}
#firehose .story .generaltitle .title a { color: #fff !important; }
